{
    "name": "ScratchX Pinball Controller",
    "description": "Interfaces to pinball controller using USB HID",
    "version": "0.0.2",
    "manifest_version": 2,
    "app": {
	"background": {
	    "scripts": ["background.js"],
	"persistent": false
	}
    },
    "icons": { "16": "pinball-16.png", "128": "pinball-128.png" },
    "key": "",
    "permissions": ["hid", {"usbDevices":
			    [ { "vendorId": 65535, "productId": 65535 } ]
			   }],
    "externally_connectable": {
	"matches": [
	    "*://scratchx.org/*",
	    "*://fluffware.github.io/*",
	    "*://mosse.local.fluffware.se/*"
	]
    }
}
